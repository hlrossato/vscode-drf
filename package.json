{
    "name": "vscode-drf",
    "displayName": "DRF Snippets",
    "description": "Django REST Framework snippets for Visual Studio Code",
    "version": "0.0.1",
    "publisher": "hlrossato",
    "icon": "images/drf-snippets.png",
    "engines": {
        "vscode": "^1.46.0"
    },
    "categories": [
        "Snippets"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/hlrossato/vscode-drf"
    },
    "contributes": {
        "snippets": [
            {
                "language": "python",
                "path": "./snippets/views/classes.json"
            },
            {
                "language": "python",
                "path": "./snippets/views/imports.json"
            },
            {
                "language": "python",
                "path": "./snippets/views/methods.json"
            },
            {
                "language": "python",
                "path": "./snippets/serializers/classes.json"
            },
            {
                "language": "python",
                "path": "./snippets/serializers/fields.json"
            },
            {
                "language": "python",
                "path": "./snippets/serializers/imports.json"
            },
            {
                "language": "python",
                "path": "./snippets/viewsets/classes.json"
            },
            {
                "language": "python",
                "path": "./snippets/viewsets/imports.json"
            }
        ]
    }
}
